          <script type="text/javascript">
          /* Table initialisation */
          oTable = null;
          $(document).ready(function() {

oTable = $('#facilityDataTable').dataTable( {
                "bAutoWidth": false,
                "bProcessing": true,
                "bServerSide": true,
                "aoColumns": [
                        <?php echo $editAction;?>
                ],
                "sAjaxSource": "<?php echo $this->url('recency',array('action' => 'index')); ?>",
                "fnServerData": function ( sSource, aoData, fnCallback ) {
                    aoData.push({"name": "fName", "value": $("#facilityName").val()});
                    aoData.push({"name": "tOutcome", "value": $("#termOut").val()});
                    aoData.push({"name": "gender", "value": $("#gender").val()});
                    aoData.push({"name": "finalOutcome", "value": $("#finalOutcome").val()});
                    aoData.push({"name": "testingFacility", "value": $("#testingFacility").val()});
                    $.ajax({
                        "dataType": 'json',
                        "type": "POST",
                        "url": sSource,
                        "data": aoData,
                        "success": fnCallback
                    });
                }
            } );


$(".showhideCheckBox").change(function(){
                if($(this).attr('checked')){
                    idpart = $(this).attr('data-showhide');
                    $("#"+idpart+"-sort").show();
                }else{
                    idpart = $(this).attr('data-showhide');
                    $("#"+idpart+"-sort").hide();
                }
            });
         
            var i = '<?php echo $i;?>';
            for(colNo=0;colNo <=i;colNo++){
                $("#iCol"+colNo).attr("checked",oTable.fnSettings().aoColumns[parseInt(colNo)].bVisible);
                
                if(oTable.fnSettings().aoColumns[colNo].bVisible){
                     $("#iCol"+colNo+"-sort").show();    
                }else{
                     $("#iCol"+colNo+"-sort").hide();    
                }
            }

            } );
            
            function closeDiv()
        {
             $("#showhide").fadeOut('slow');
        }

        function fnShowHide(iCol)
        {
            var bVis = oTable.fnSettings().aoColumns[iCol].bVisible;
                oTable.fnSetColumnVis( iCol, bVis ? false : true );
        }
        
          function searchVlRequestData(){
            $.blockUI();
            oTable.fnDraw();
            $.unblockUI();
        }
        
        function exportRecencyData()
        {
            $.blockUI();
            $.post("<?php echo $this->url('recency',array('action'=>'export-recency')); ?>",
                function(data){
                if(data == "" || data == null || data == undefined){
                $.unblockUI();
                alert('Unable to generate download');
                }else{
                $.unblockUI();
                document.location.href = '/temporary/'+data;
                }
            });
        }

        function generatePdf(recencyId)
        {
            $.blockUI();
            $.post("<?php echo $this->url('recency',array('action'=>'generate-pdf')); ?>",{recencyId:recencyId},
                function(data){
                if(data == "" || data == null || data == undefined){
                $.unblockUI();
                alert('Unable to generate download');
                }else{
                $.unblockUI();
                window.open('/temporary/'+data,'blank');
                //document.location.href = '/temporary/'+data;
                }
            });
        }

