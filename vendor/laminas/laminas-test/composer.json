{
    "name": "laminas/laminas-test",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "test"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-test/",
        "issues": "https://github.com/laminas/laminas-test/issues",
        "source": "https://github.com/laminas/laminas-test",
        "rss": "https://github.com/laminas/laminas-test/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6-dev",
            "dev-develop": "2.7-dev"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "laminas/laminas-console": "^2.6",
        "laminas/laminas-dom": "^2.6",
        "laminas/laminas-eventmanager": "^2.6.2 || ^3.0",
        "laminas/laminas-http": "^2.5.4",
        "laminas/laminas-mvc": "^2.7.1",
        "laminas/laminas-servicemanager": "^2.7.5 || ^3.0.3",
        "laminas/laminas-stdlib": "^2.7 || ^3.0",
        "laminas/laminas-uri": "^2.5",
        "laminas/laminas-view": "^2.6.3",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "phpunit/phpunit": "^4.0 || ^5.0",
        "sebastian/version": "^1.0.4"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-i18n": "^2.6",
        "laminas/laminas-log": "^2.7.1",
        "laminas/laminas-modulemanager": "^2.7.1",
        "laminas/laminas-serializer": "^2.6.1",
        "laminas/laminas-session": "^2.6.2",
        "mikey179/vfsstream": "~1.2",
        "symfony/finder": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Test\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Test\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-test": "self.version"
    }
}
